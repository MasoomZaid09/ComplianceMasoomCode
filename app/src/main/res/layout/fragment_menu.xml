                    android:layout_marginRight="30dp"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/StandardCompliance"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtStandardComplience"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Standard Compliance"
                            android:textColor="#0A3383"
                            android:textSize="13dp"
                            tools:layout_editor_absoluteX="2dp"
                            tools:layout_editor_absoluteY="115dp" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#D2D6DE" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/NewCompliance"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="New Compliance"
                            android:textColor="#0A3383"
                            android:textSize="13dp"
                            tools:layout_editor_absoluteX="2dp"
                            tools:layout_editor_absoluteY="115dp" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#D2D6DE" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/UpdateCompliance"
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Update Compliance"
                            android:textColor="#0A3383"
                            android:textSize="13dp"
                            tools:layout_editor_absoluteX="2dp"
                            tools:layout_editor_absoluteY="115dp" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:background="#D2D6DE" />
                    </LinearLayout>


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/helpAndSupport"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="25dp"
                        android:src="@drawable/support" />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="13dp"
                        android:layout_marginRight="10dp"
                        android:gravity="left"
                        android:text="Help &amp; Support "
                        android:textColor="#0A3383"
                        android:textSize="15dp"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/settings"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25dp"
                        android:src="@drawable/ic_settings"
                        app:tint="@color/black"/>

                    <TextView
                        android:id="@+id/txtSettings"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="11dp"
                        android:layout_marginRight="10dp"
                        android:gravity="left"
                        android:text="Settings"
                        android:textColor="#0A3383"
                        android:textSize="18sp"
                        android:textStyle="bold" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:background="#D2D6DE" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/logout"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginTop="5dp"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_marginLeft="27dp"
                        android:src="@drawable/logout" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="11dp"
                        android:layout_marginRight="10dp"
                        android:gravity="left"
                        android:text="Logout"
                        android:textColor="#0A3383"
                        android:textSize="15dp"
                        android:textStyle="bold" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="0.5dp"
                        android:background="#D2D6DE" />

                </LinearLayout>

            </LinearLayout>


        </LinearLayout>

    </androidx.core.widget.NestedScrollView>

</LinearLayout>